{
  "name": "api",
  "version": "0.0.0",
  "description": "hugofqueiros api",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --inspect src/index.js --exec babel-node --presets es2015,stage-2",
    "start2": "nodemon --debug src/index.js --exec babel-node --presets es2015,stage-2",
    "start3": "node src/index.js",
    "node:inspect": "node-inspector",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "test": "mocha --compilers js:babel-register",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "release:patch": "npm version patch -m \"chore(release): Bump to %s\"",
    "release:minor": "npm version minor -m \"chore(release): Bump to %s\"",
    "release:major": "npm version major -m \"chore(release): Bump to %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugofqueiros/api.git"
  },
  "author": "Hugo Queiros <hugofqueiros@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hugofqueiros/api/issues"
  },
  "homepage": "https://github.com/hugofqueiros/api#readme",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-reverse": "^0.3.2",
    "express-session": "^1.14.2",
    "helmet": "^3.4.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.13.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "snyk": "^1.24.4"
  }
}
